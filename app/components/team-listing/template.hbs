<li class="team-listing-li">
  <div class="team-name">
    {{#if team.name}}
      <button class='js-delete-team button__x' {{action 'deleteTeam'}}>&#215;</button> <span class="title">{{team.name}}</span>
    {{else}}
      <span class="title">Free Agentz</span>
    {{/if}}
  </div>
  <ul class="engineer-list">
  {{#each team.engineers as |engineer|}}
    {{engineer-listing engineer=engineer deleteEngineer=(action 'deleteEngineer' engineer)}}
  {{/each}}
  </ul>
</li>
